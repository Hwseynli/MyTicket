{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Concole", "Serilog.Sinks.File" ],
        "MinimumLevel": "Error",
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "/Users/zeyneb/home/log/ticketlogs.txt",
                    "rollingInterval": "Day"
                }
            }
        ],
        "Properties": {
            "ApplicationName": "MyTicket"
        },
        "AllowedHosts": "*"
    }
}
